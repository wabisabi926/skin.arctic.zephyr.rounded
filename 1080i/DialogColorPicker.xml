<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">6</defaultcontrol>

    <controls>
        <include>Background_Overlay</include>
        <control type="group">

            <include content="Def_Dialog_Background"><param name="height" value="820" /><param name="width" value="1480" /></include>

            <!-- HEADER -->

            <control type="label" id="1">
                <include>Def_Dialog_Header_Title</include>
                <label>31472</label>
            </control>

            <include>Def_Dialog_Header_Logo</include>

            <control type="image">
                <top>95</top>
                <left>30</left>
                <width>538</width>
                <height>1</height>
                <bottom>10</bottom>
                <colordiffuse>Black12</colordiffuse>
                <texture>common/white.png</texture>
            </control>

            <!-- LINE VERTICAL -->

            <control type="image">
                <top>10</top>
                <left>600</left>
                <width>1</width>
                <bottom>10</bottom>
                <colordiffuse>Black12</colordiffuse>
                <texture>common/white.png</texture>
            </control>

            <!-- COLOR PICKER PANEL -->

           <control type="group">
                <right>20</right>
                <top>20</top>
                <width>840</width>
                <height>780</height>
                <orientation>vertical</orientation>
                <onleft>3100</onleft>
                <onback>3100</onback>
                <control type="panel" id="6">
                    <pagecontrol>61</pagecontrol>
                    <onleft>9002</onleft>
                    <onback>9002</onback>
                    <itemlayout height="60" width="60">
                        <include content="Object_Control">
                            <param name="control" value="image" />
                            <include content="Def_Padding"><param name="pad">1</param></include>
                            <texture>common/white.png</texture>
                            <colordiffuse>$INFO[ListItem.Label2]</colordiffuse>
                        </include>
                    </itemlayout>
                    <focusedlayout height="60" width="60">
                        <include content="Object_Control">
                            <param name="control" value="image" />
                            <include content="Def_Padding"><param name="pad">1</param></include>
                            <texture>common/white.png</texture>
                            <colordiffuse>$INFO[ListItem.Label2]</colordiffuse>
                        </include>
                    </focusedlayout>
                </control>

                <!-- SELECTION BOX -->

                <control type="group">
                    <left>-20</left>
                    <top>-20</top>
                    <width>100</width>
                    <height>100</height>
                    <visible>Control.Hasfocus(6)</visible>
                    <animation effect="slide" end="0,60" condition="Container(6).Row(1)">Conditional</animation>
                    <animation effect="slide" end="0,120" condition="Container(6).Row(2)">Conditional</animation>
                    <animation effect="slide" end="0,180" condition="Container(6).Row(3)">Conditional</animation>
                    <animation effect="slide" end="0,240" condition="Container(6).Row(4)">Conditional</animation>
                    <animation effect="slide" end="0,300" condition="Container(6).Row(5)">Conditional</animation>
                    <animation effect="slide" end="0,360" condition="Container(6).Row(6)">Conditional</animation>
                    <animation effect="slide" end="0,420" condition="Container(6).Row(7)">Conditional</animation>
                    <animation effect="slide" end="0,480" condition="Container(6).Row(8)">Conditional</animation>
                    <animation effect="slide" end="0,540" condition="Container(6).Row(9)">Conditional</animation>
                    <animation effect="slide" end="0,600" condition="Container(6).Row(10)">Conditional</animation>
                    <animation effect="slide" end="0,660" condition="Container(6).Row(11)">Conditional</animation>
                    <animation effect="slide" end="0,720" condition="Container(6).Row(12)">Conditional</animation>
                    <!-- <animation effect="slide" end="0,780" condition="Container(6).Row(13)">Conditional</animation> -->


                    <animation effect="slide" end="60" condition="Container(6).Column(1)">Conditional</animation>
                    <animation effect="slide" end="120" condition="Container(6).Column(2)">Conditional</animation>
                    <animation effect="slide" end="180" condition="Container(6).Column(3)">Conditional</animation>
                    <animation effect="slide" end="240" condition="Container(6).Column(4)">Conditional</animation>
                    <animation effect="slide" end="300" condition="Container(6).Column(5)">Conditional</animation>
                    <animation effect="slide" end="360" condition="Container(6).Column(6)">Conditional</animation>
                    <animation effect="slide" end="420" condition="Container(6).Column(7)">Conditional</animation>
                    <animation effect="slide" end="480" condition="Container(6).Column(8)">Conditional</animation>
                    <animation effect="slide" end="540" condition="Container(6).Column(9)">Conditional</animation>
                    <animation effect="slide" end="600" condition="Container(6).Column(10)">Conditional</animation>
                    <animation effect="slide" end="660" condition="Container(6).Column(11)">Conditional</animation>
                    <animation effect="slide" end="720" condition="Container(6).Column(12)">Conditional</animation>
                    <animation effect="slide" end="780" condition="Container(6).Column(13)">Conditional</animation>
                    <animation effect="slide" end="840" condition="Container(6).Column(14)">Conditional</animation>
                    <animation effect="slide" end="900" condition="Container(6).Column(15)">Conditional</animation>
                    <animation effect="slide" end="960" condition="Container(6).Column(16)">Conditional</animation>
                    <animation effect="slide" end="1020" condition="Container(6).Column(17)">Conditional</animation>

                    <include content="Object_Control">
                        <param name="control" value="group" />
                        <include content="Def_Padding"><param name="pad">5</param></include>
                        <control type="image">
                            <include>Texture_Shadow_20</include>
                            <texture border="20" colordiffuse="ccffffff">common/box.png</texture>
                        </control>
                    </include>
                    <control type="image">
                        <bordersize>10</bordersize>
                        <texture colordiffuse="$INFO[Container(6).ListItem.Label2]" border="12">common/box.png</texture>
                    </control>
                </control>

                <include content="Dialog_Scrollbar">
                    <onright>6</onright>
                    <onleft>9002</onleft>
                </include>
            </control>

            <!-- LISTS, BUTTONS -->

            <control type="grouplist" id="9000">
                <description>Control Area</description>
                <left>0</left>
                <width>600</width>
                <top>112</top>
                <height>596</height>
                <orientation>vertical</orientation>
                <pagecontrol>60</pagecontrol>
                <usecontrolcoords>true</usecontrolcoords>
                <itemgap>0</itemgap>
                <control type="button" id="3300">
                    <disabledcolor>PanelWhite100</disabledcolor>
                    <width>600</width>
                    <align>left</align>
                    <font>Small</font>
                    <textcolor>PanelWhite100</textcolor>
                    <disabledcolor>PanelWhite12</disabledcolor>
                    <height>70</height>
                    <width>450</width>
                    <alttexturefocus colordiffuse="$VAR[Color_Highlight]">common/white.png</alttexturefocus>
                    <texturefocus colordiffuse="$VAR[Color_Highlight]">common/white.png</texturefocus>
                    <label>$LOCALIZE[737]:</label>
                    <label2>$INFO[Container(6).ListItem.Label2,#,]</label2>
                    <enable>false</enable>
                </control>
                <control type="button" id="3500">
                    <disabledcolor>PanelWhite100</disabledcolor>
                    <width>600</width>
                    <align>left</align>
                    <font>Small</font>
                    <textcolor>PanelWhite100</textcolor>
                    <disabledcolor>PanelWhite12</disabledcolor>
                    <height>70</height>
                    <width>450</width>
                    <alttexturefocus colordiffuse="$VAR[Color_Highlight]">common/white.png</alttexturefocus>
                    <texturefocus colordiffuse="$VAR[Color_Highlight]">common/white.png</texturefocus>
                    <label>$LOCALIZE[551]:</label>
                    <label2>$INFO[Container(6).ListItem.Label]</label2>
                    <visible>!String.IsEmpty(Container(6).ListItem.Label)</visible>
                    <enable>false</enable>
                </control>
            </control>

            <!-- BUTTON ON BOTTOM -->

            <control type="grouplist" id="9002">
                <bottom>30</bottom>
                <height>66</height>
                <left>150</left>

                <onright>6</onright>
                <onleft>6</onleft>
                <onup>6</onup>
                <ondown>6</ondown>
                <onclick>close</onclick>

                <itemgap>14</itemgap>
                <orientation>horizontal</orientation>

                <include content="Def_Dialog_Button_Action">
                    <param name="id" value="18" />
                    <param name="label" value="$LOCALIZE[15067]" />
                </include>
            </control>
        </control>
    </controls>
</window>

    <!-- ALBUM / DISC - COUNT -->

    <include name="Number_Album_Disc_Count">
        <control type="group">
            <visible>Container.Content(albums) | String.IsEqual(ListItem.DBType,album)</visible>
            <centertop>50%</centertop>
            <width>180</width>
            <height>40</height>
            <control type="image" description="button back">
                <width>45</width>
                <height>45</height>
                <left>10</left>
                <align>center</align>
                <texture border="0" colordiffuse="$VAR[Color_Background]">common/circle.png</texture>
            </control>
            <control type="label">
                <width>45</width>
                <height>40</height>
                <align>center</align>
                <left>10</left>
                <centertop>53%</centertop>
                <label>$VAR[Label_Album_Disc_Count]</label>
                <font>Flag</font>
                <textcolor>Dark1</textcolor>
            </control>
            <control type="label">
                <width>auto</width>
                <height>40</height>
                <align>center</align>
                <left>60</left>
                <centertop>53%</centertop>
                <label>$LOCALIZE[134]</label>
                <font>Flag</font>
                <textcolor>Dark1</textcolor>
                <visible>!Integer.IsGreater(ListItem.Property(Album_Totaldiscs),1) | String.StartsWith(ListItem.Label,$LOCALIZE[427])</visible>
            </control>
            <control type="label">
                <width>auto</width>
                <height>40</height>
                <align>center</align>
                <left>60</left>
                <centertop>53%</centertop>
                <label>$LOCALIZE[14087]</label>
                <font>Flag</font>
                <textcolor>Dark1</textcolor>
                <visible>Integer.IsGreater(ListItem.Property(Album_Totaldiscs),1) + !String.StartsWith(ListItem.Label,$LOCALIZE[427])</visible>
            </control>
        </control>
    </include>


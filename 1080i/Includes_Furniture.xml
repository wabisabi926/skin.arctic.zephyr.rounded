    <!-- FURNITURE - HEADER - BUSY SPINNER -->

    <include name="Furniture_Header_Busy">
        <control type="group">
            <top>SidePad</top>
            <left>SidePad</left>
            <visible>Skin.HasSetting(hide.busydialog) + Window.IsVisible(DialogBusy.xml)</visible>

            <animation type="Conditional" condition="Skin.HasSetting(hide.busydialog) + Window.IsVisible(DialogBusy.xml)" reversible="true">
                <effect type="slide" start="-400,0" end="0,0" tween="sine" easing="in" time="450" delay="850"/>
                <effect type="fade" start="0" end="100" time="450" delay="850"/>
            </animation>

            <include content="Object_Spinner_Busy">
                <param name="haslabel" value="false" />
                <param name="centerleft" value="44" />
                <param name="centertop" value="40" />
            </include>
             <control type="label">
                <left>108</left>
                <align>left</align>
                <centertop>24</centertop>
                <width>auto</width>
                <font>HeaderLabel</font>
                <textcolor>Dark1</textcolor>
                <label>$LOCALIZE[20186]</label>
            </control>
            <control type="label">
                <left>108</left>
                <align>left</align>
                <centertop>58</centertop>
                <font>Tiny</font>
                <width>auto</width>
                <textcolor>Dark2</textcolor>
                <label>$LOCALIZE[31664]...</label>
            </control>
        </control>
    </include>


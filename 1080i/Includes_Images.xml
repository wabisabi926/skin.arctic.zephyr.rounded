    <!-- IMAGE - FANART -->

    <variable name="Image_Fanart">

        <!-- set -->
        <value condition="!String.IsEmpty(ListItem.Art(landscape)) + String.IsEqual(ListItem.DBType,set)">$INFO[ListItem.Art(landscape)]</value>
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + String.IsEqual(ListItem.DBType,set)">$INFO[ListItem.Art(fanart)]</value>

        <!-- tvshows -->
        <value condition="!String.IsEmpty(ListItem.Art(landscape)) + Container.Content(tvshows) + !Skin.HasSetting(dont.use.landscape.tvshows)">$INFO[ListItem.Art(landscape)]</value>
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content(tvshows)">$INFO[ListItem.Art(fanart)]</value>

        <!-- season -->
        <value condition="!String.IsEmpty(ListItem.Art(tvshow.landscape)) + Container.Content(seasons) + !Skin.HasSetting(dont.use.landscape.seasons)">$INFO[ListItem.Art(tvshow.landscape)]</value>
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content(seasons)">$INFO[ListItem.Art(fanart)]</value>

        <!-- View 516 seasons -->
        <value condition="!String.IsEmpty(Container(5161).ListItem.Icon) + String.IsEqual(Container(5161).ListItem.DBType,episode) + Control.HasFocus(5161)">$INFO[Container(5161).ListItem.Icon]</value>

        <!-- movie -->
        <value condition="!String.IsEmpty(ListItem.Art(landscape)) + Container.Content(movies) + !Skin.HasSetting(dont.use.landscape.movies)">$INFO[ListItem.Art(landscape)]</value>
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content(movies) + String.IsEqual(ListItem.DBType,movie)">$INFO[ListItem.Art(fanart)]</value>

        <!-- addon -->
        <value condition="!String.IsEmpty(ListItem.Icon) + Container.Content(addons)">$INFO[ListItem.Icon]</value>

        <!-- game mode -->
        <value condition="!String.IsEmpty(ListItem.Art(title)) + $EXP[IsGameMode]">$INFO[ListItem.Art(title)]</value>

        <!-- fanart - songs / albums -->
        <value condition="!String.IsEmpty(ListItem.Art(artist.fanart)) + [Container.Content(songs) | Container.Content(albums)]">$INFO[ListItem.Art(artist.fanart)]</value>

        <!-- fanart - artists -->
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content(artists)">$INFO[ListItem.Art(fanart)]</value>

        <!-- fanart - plugin -->
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content() + !$EXP[IsYoutubeItem] ">$INFO[ListItem.Art(fanart)]</value>

        <!-- Thumb - Youtube -->
        <value condition="!String.IsEmpty(ListItem.Thumb) + Container.Content(movies) + $EXP[IsYoutubeItem]">$INFO[ListItem.Thumb]</value>
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content() + $EXP[IsYoutube]">$INFO[ListItem.Art(fanart)]</value>
        <!-- <value condition="!String.IsEmpty(ListItem.Icon) + Container.Content(files) + $EXP[IsYoutubeItem]">$INFO[ListItem.Icon]</value> -->

        <!-- fanart - TMDb Helper -->
        <value condition="!String.IsEmpty(ListItem.Art(fanart)) + Container.Content(movies) + $EXP[IsTMDbHelperItem]">$INFO[ListItem.Art(fanart)]</value>

        <!-- Thumb - all others  -->
        <value condition="!String.IsEmpty(ListItem.Thumb)">$INFO[ListItem.Thumb]</value>

        <!-- PVR epg event-->
        <value condition="!String.IsEmpty(ListItem.EpgEventIcon) + String.StartsWith(ListItem.FolderPath,pvr://timers)">$INFO[ListItem.EpgEventIcon]</value>

        <!-- all others -->
        <value condition="String.IsEqual(ListItem.DBType,genre) | String.EndsWith(Container.FolderPath,Genres)">$VAR[Image_Thumb_Genres]</value>
        <value condition="String.IsEqual(ListItem.DBType,country)">$VAR[Image_Thumb_Countries]</value>
        <value condition="String.IsEqual(ListItem.DBType,studio)">$VAR[Image_Thumb_Studios]</value>
        <value condition="String.IsEqual(ListItem.DBType,director)">DefaultDirector.png</value>
        <value condition="String.IsEqual(ListItem.DBType,tag)">DefaultTags.png</value>
        <value condition="String.IsEqual(ListItem.DBType,set)">DefaultSets.png</value>
        <value condition="Container.Content(years)">DefaultYear.png</value>

        <value>$INFO[ListItem.Icon]</value>

        <!-- <value condition="!String.IsEmpty(ListItem.Art(landscape)) + Container.Content(seasons) + !Skin.HasSetting(dont.use.landscape.seasons)">$INFO[ListItem.Art(landscape)]</value> -->

    </variable>


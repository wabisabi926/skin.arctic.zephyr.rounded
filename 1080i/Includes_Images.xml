    <variable name="Image_Addons_Status_Icon">
        <value condition="[String.IsEqual(ListItem.AddonLifecycleType,$LOCALIZE[24170]) | String.IsEqual(ListItem.AddonLifecycleType,$LOCALIZE[24171])] + !ListItem.Property(addon.isenabled)">addonstatus/disable.png</value>
        <value condition="ListItem.Property(addon.orphaned)">addonstatus/orphan.png</value>
        <value condition="ListItem.Property(addon.downloading)">addonstatus/install.png</value>
        <!-- <value condition="ListItem.Property(addon.isinstalled) + !ListItem.Property(addon.isenabled) + Window.IsActive(addonbrowser)">addonstatus/disable.png</value> -->
        <value condition="ListItem.Property(addon.hasupdate) | ListItem.Property(addon.isupdate)">addonstatus/update.png</value>
        <value condition="ListItem.Property(addon.isenabled) + String.IsEqual(ListItem.AddonLifecycleType,$LOCALIZE[24169])">addonstatus/enabled-normal.png</value>
        <value condition="ListItem.Property(addon.isenabled) + String.IsEqual(ListItem.AddonLifecycleType,$LOCALIZE[24170])">addonstatus/enabled-deprecated.png</value>
        <value condition="ListItem.Property(addon.isenabled) + String.IsEqual(ListItem.AddonLifecycleType,$LOCALIZE[24171])">addonstatus/enabled-broken.png</value>
        <!-- <value condition="!ListItem.IsParentFolder">addonstatus/OverlayUnwatched.png</value> -->
    </variable>

    <variable name="Image_Addons_Status_Icon_Disable">
        <value condition="ListItem.Property(addon.isinstalled) + !ListItem.Property(addon.isenabled) + Window.IsActive(addonbrowser)">addonstatus/disable.png</value>
    </variable>

    <variable name="Image_Addons_Origin_Icon">
        <value condition="ListItem.Property(Addon.IsFromOfficialRepo) + ListItem.IsAutoUpdateable">addonstatus/official.png</value>
        <value condition="ListItem.Property(Addon.IsFromOfficialRepo)">addonstatus/official-pinned.png</value>
        <value condition="String.IsEqual(ListItem.AddonOrigin,$LOCALIZE[25014]) + ListItem.IsAutoUpdateable">addonstatus/manual.png</value>
        <value condition="String.IsEqual(ListItem.AddonOrigin,$LOCALIZE[25014])">addonstatus/manual-pinned.png</value>
        <value condition="ListItem.IsAutoUpdateable">addonstatus/install.png</value>
        <value>addonstatus/install-pinned.png</value>
    </variable>

    <!-- IMAGE - OSD -->

    <variable name="Image_OSD_UserRating">
        <value condition="!String.IsEmpty(MusicPlayer.UserRating)">osd/rating_new.png</value>

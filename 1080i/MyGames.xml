                    <onclick condition="Skin.HasSetting(enable.items.focus.glow) + Skin.HasSetting(enable.items.focus.glow.high)">Skin.Reset(enable.items.focus.glow)</onclick>
                    <onclick condition="Skin.HasSetting(enable.items.focus.glow) + Skin.HasSetting(enable.items.focus.glow.high)">Skin.Reset(enable.items.focus.glow.high)</onclick>

<?xml version="1.0" encoding="UTF-8"?>
<includes>

    <include name="View_500_Thumbnails">
        <control type="group">
            <visible>Control.IsVisible(500)</visible>

            <include condition="!Skin.HasSetting(furniture.weather.home.only) + !Skin.HasSetting(show.furniture.footer)">Furniture_Footer_Clock_Right</include>
            <include>Animation_Common</include>
            <include>View_Slide_For_JumpBar</include>

            <control type="group">
                <control type="panel" id="500">
                    <visible>$EXP[Exp_View_500]</visible>

                    <include>Def_On_Info</include>

                    <top>206</top>
                    <left>70</left>
                    <width>1800</width>
                    <height>680</height>
                    <onleft condition="!Skin.HasSetting(global.kioskmode)">9000</onleft>
                    <onleft condition="!Skin.HasSetting(global.kioskmode) + [Player.HasMedia | Player.Playing]">AlarmClock(GoToButtonFullScreen,SetFocus(6157),00:00,silent)</onleft>
                    <onright>1635</onright>
                    <onup>500</onup>
                    <ondown>500</ondown>
                    <preloaditems>0</preloaditems>
                    <pagecontrol>1635</pagecontrol>
                    <orientation>vertical</orientation>

                    <viewtype label="21371">list</viewtype>
                    <include>Animation_Scrolltime_Views_List</include>

                    <itemlayout height="340" width="299.33">
                        <control type="group">
                            <height>280</height>
                            <width>280</width>
                            <include content="Layout_Widget_Square">
                                <param name="id" value="500"/>
                                <param name="height" value="280"/>
                                <param name="distance" value="10"/>
                                <param name="aspectratio" value="scale"/>
                            </include>
                        </control>
                        <control type="label">
                            <left>15</left>
                            <right>30</right>
                            <bottom>24</bottom>
                            <height>50</height>
                            <align>center</align>
                            <font>InfoNetflix</font>
                            <label>$VAR[Label_Widget_Lign_1]</label>
                            <textcolor>Dark1</textcolor>
                            <selectedcolor>Dark1</selectedcolor>
                        </control>
                        <control type="label">
                            <left>15</left>
                            <right>30</right>
                            <bottom>-10</bottom>
                            <height>50</height>
                            <align>center</align>
                            <font>Small24</font>
                            <label>$VAR[Label_Widget_Lign_2]</label>
                            <textcolor>PanelWhite70</textcolor>
                            <selectedcolor>PanelWhite70</selectedcolor>
                        </control>
                    </itemlayout>
                    <focusedlayout height="340" width="299.33">
                        <control type="group">
                            <height>280</height>
                            <width>280</width>
                            <include content="Layout_Widget_Square_Focus">
                                <param name="id" value="500"/>
                                <param name="height" value="280"/>
                                <param name="distance" value="10"/>
                                <param name="aspectratio" value="scale"/>
                                <param name="glow_height" value="280" />
                                <param name="glow_top" value="0" />
                            </include>
                        </control>
                        <control type="label">
                            <left>15</left>
                            <right>30</right>
                            <bottom>24</bottom>
                            <height>50</height>
                            <align>center</align>
                            <font>InfoNetflix</font>
                            <label>$VAR[Label_Widget_Lign_1]</label>
                            <textcolor>Dark1</textcolor>
                            <selectedcolor>Dark1</selectedcolor>
                        </control>
                        <control type="label">
                            <left>15</left>
                            <right>30</right>
                            <bottom>-10</bottom>
                            <height>50</height>
                            <align>center</align>
                            <font>Small24</font>
                            <label>$VAR[Label_Widget_Lign_2]</label>
                            <textcolor>PanelWhite70</textcolor>
                            <selectedcolor>PanelWhite70</selectedcolor>
                        </control>
                    </focusedlayout>
                </control>
            </control>
            <control type="scrollbar" id="1635">
                <include content="Object_Scrollbar_Views_Vertical">
                    <param name="scrollid" value="1635"/>
                    <param name="idlayout" value="500"/>
                </include>
            </control>

            <include content="Def_Hidden_List_Artist_Album_Count"><param name="id" value="500" /></include>
        </control>
    </include>


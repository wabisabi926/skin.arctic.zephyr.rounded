<?xml version="1.0" encoding="UTF-8"?>
<includes>

    <include name="View_513_Shifted">
        <description>List View (id=513)</description>
        <control type="group">
            <include condition="!Skin.HasSetting(furniture.weather.home.only) + !Skin.HasSetting(show.furniture.footer)">Furniture_Footer_Clock_Right</include>

            <include>Animation_Common</include>
            <visible>Control.IsVisible(513)</visible>
            <include>View_Slide_For_JumpBar</include>

            <control type="panel" id="513">
                <visible>$EXP[Exp_View_513]</visible>
                <top>200</top>
                <right>SidePad</right>
                <left>70</left>
                <onleft condition="!Skin.HasSetting(global.kioskmode)">9000</onleft>
                <onright>1689</onright>
                <preloaditems>1</preloaditems>
                <pagecontrol>1689</pagecontrol>
                <height>690</height>
                <orientation>vertical</orientation>
                <viewtype label="31081">list</viewtype>
                <include>Animation_Scrolltime_Views_List</include>
                <itemlayout height="230">
                    <control type="group">
                        <height>225</height>
                        <width>390</width>
                        <include content="Layout_Widget_Landscape">
                            <param name="id" value="513"/>
                            <param name="height" value="225"/>
                        </include>
                    </control>
                    <control type="label">
                        <top>16</top>
                        <left>410</left>
                        <description>Header Label</description>
                        <textcolor>Dark1</textcolor>
                        <selectedcolor>Dark1</selectedcolor>
                        <font>SmallBold</font>
                        <width>1350</width>
                        <label fallback="19055">$INFO[ListItem.Label]</label>
                    </control>
                    <control type="label">
                        <visible>!ListItem.IsParentFolder</visible>
                        <top>71</top>
                        <left>410</left>
                        <height>36</height>
                        <font>Tiny</font>
                        <textcolor>Dark1</textcolor>
                        <selectedcolor>Dark1</selectedcolor>
                        <width>1350</width>
                        <label>$VAR[513_Row_1]</label>
                    </control>
                    <control type="textbox">
                        <top>126</top>
                        <bottom>17</bottom>
                        <left>410</left>
                        <font>Tiny</font>
                        <height>76</height>
                        <textcolor>Dark2</textcolor>
                        <selectedcolor>Dark2</selectedcolor>
                        <label>$INFO[ListItem.Plot]</label>
                        <width>1330</width>
                        <autoscroll>false</autoscroll>
                    </control>
                </itemlayout>
                <focusedlayout height="230">
                    <control type="group">
                        <height>225</height>
                        <width>390</width>
                        <include content="Layout_Widget_Landscape_Focus">
                            <param name="id" value="513"/>
                            <param name="height" value="225"/>
                            <param name="glow_top" value="0"/>
                            <param name="glow_height" value="225"/>
                            <param name="glow_left" value="4" />
                            <param name="glow_right" value="5" />
                        </include>
                    </control>
                    <control type="label">
                        <top>16</top>
                        <left>410</left>
                        <description>Header Label</description>
                        <textcolor>Dark1</textcolor>
                        <selectedcolor>Dark1</selectedcolor>
                        <font>SmallBold</font>
                        <width>1350</width>
                        <label fallback="19055">$INFO[ListItem.Label]</label>
                    </control>
                    <control type="group">
                        <visible>!ListItem.IsParentFolder</visible>
                        <top>71</top>
                        <left>410</left>
                        <control type="label">
                            <height>36</height>
                            <font>Tiny</font>
                            <textcolor>Dark1</textcolor>
                            <selectedcolor>Dark1</selectedcolor>
                            <width>1350</width>
                            <label>$VAR[Label_Duration_Var_50]</label>
                            <include>Animation_TestIn</include>
                        </control>
                        <control type="label">
                            <height>36</height>
                            <font>Tiny</font>
                            <textcolor>Dark1</textcolor>
                            <selectedcolor>Dark1</selectedcolor>
                            <width>1350</width>
                            <label>$VAR[513_Row_1]</label>
                            <include>Animation_TestOut</include>
                        </control>
                    </control>
                    <control type="textbox">
                        <top>126</top>
                        <bottom>17</bottom>
                        <left>410</left>
                        <font>Tiny</font>
                        <height>76</height>
                        <textcolor>Dark2</textcolor>
                        <selectedcolor>Dark2</selectedcolor>
                        <label>$INFO[ListItem.Plot]</label>
                        <width>1330</width>
                    </control>
                </focusedlayout>
            </control>
            <control type="scrollbar" id="1689">
                <include content="Object_Scrollbar_Vertical">
                    <param name="scrollid" value="1689"/>
                    <param name="idlayout" value="513"/>
                </include>
            </control>
            <control type="image">
                <visible>Control.HasFocus(513) + !Container(513).OnPrevious</visible>
                <include>Animation_Delay_FadeIn</include>
                <top>176</top>
                <left>258</left>
                <include>arrowdimensions</include>
                <texture colordiffuse="Dark1" flipy="true">common/arrow-small.png</texture>
            </control>
            <control type="image">
                <visible>Control.HasFocus(513) + !Container(513).OnNext</visible>
                <include>Animation_Delay_FadeIn</include>
                <top>891</top>
                <left>258</left>
                <include>arrowdimensions</include>
                <texture colordiffuse="Dark1" flipy="false">common/arrow-small.png</texture>
            </control>
        </control>
    </include>
</includes>

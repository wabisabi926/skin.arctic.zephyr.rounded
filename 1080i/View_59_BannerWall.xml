<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="View_59_BannerWall">
        <description>List View (id=59)</description>
        <control type="group">
            <visible>Control.IsVisible(59)</visible>
            <include>Animation_Common</include>

            <include condition="!Skin.HasSetting(show.clock.date.weather.home.only) + !Skin.HasSetting(show.furniture.footer)">Furniture_Footer_Clock_Right</include>
            <include>View_Slide_For_JumpBar</include>

            <control type="panel" id="59">
                <visible>$EXP[Exp_View_59]</visible>
                <include>Animation_Scrolltime_Views_List</include>

                <top>200</top>
                <right>70</right>
                <left>70</left>
                <height>666</height>

                <include>Def_Focus_Context_Menu_On_Right</include>
                <onleft>1619</onleft>
                <onup>59</onup>
                <ondown>59</ondown>

                <preloaditems>2</preloaditems>
                <pagecontrol>1619</pagecontrol>

                <viewtype label="31215">list</viewtype>

                <itemlayout height="133" width="593">
                    <control type="group">
                        <height>128</height>
                        <top>8</top>
                        <include content="Layout_Widget_Banner">
                            <param name="texture_diffuse" value="banner_w574_h108" />
                            <param name="id" value="59"/>
                            <param name="height" value="128"/>
                        </include>
                    </control>
                </itemlayout>
                <focusedlayout height="133" width="593">
                    <control type="group">
                        <height>128</height>
                        <top>8</top>
                        <include content="Layout_Widget_Banner_Focus">
                            <param name="texture_diffuse" value="banner_w574_h108" />
                            <param name="id" value="59"/>
                            <param name="height" value="128"/>
                            <param name="glow_height" value="135"/>
                            <param name="glow_top" value="-4"/>
                            <param name="glow_left" value="11" />
                            <param name="glow_right" value="13" />
                        </include>
                    </control>
                </focusedlayout>
            </control>

            <!-- SCROLLBAR -->

            <control type="scrollbar" id="1619">
                <include content="Object_Scrollbar_Views_Vertical">
                    <param name="scrollid" value="1619" />
                    <param name="idlayout" value="59" />
                </include>
            </control>
        </control>
    </include>
</includes>
